{
  "name": "workspace-manager",
  "version": "2.0.0",
  "description": "Advanced workspace management system with real-time monitoring, dependency analysis, and visual topology mapping",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node start.js",
    "dev": "nodemon src/index.js",
    "build": "webpack --mode production",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "serve": "http-server dist -p 8080",
    "install-deps": "npm install",
    "setup": "node start.js --setup",
    "service:pm2": "node start.js --pm2",
    "service:systemd": "node start.js --systemd",
    "service:all": "node start.js --service"
  },
  "keywords": [
    "workspace",
    "monitoring",
    "topology",
    "visualization",
    "dependency-analysis"
  ],
  "author": "DevCore Team",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "acorn": "^8.11.2",
    "acorn-walk": "^8.3.0",
    "typescript": "^5.3.3",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "markdown-it": "^14.0.0",
    "yaml": "^2.3.4",
    "axios": "^1.6.2",
    "node-fetch": "^3.3.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "winston": "^3.11.0",
    "systeminformation": "^5.21.20"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "pm2": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "http-server": "^14.1.1"
  }
}
