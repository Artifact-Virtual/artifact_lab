{
  "paths": {
    "src": "./src",
    "tests": "./tests",
    "docs": "./docs",
    "workspace": "./workspace"
  },
  "modelSettings": {
    "defaultModel": "codellama:7b",
    "modelEndpoint": "http://localhost:11434/api/generate",
    "provider": "ollama"
  },
  "logging": {
    "level": "info",
    "output": "console"
  },
  "visualization": {
    "enable3D": true,
    "defaultView": "tree"
  },
  "fileWatcher": {
    "debounceTime": 300
  },
  "agents": {
    "maxRetries": 5,
    "timeout": 30
  }
}